#!/usr/bin/env bash


echo 'Cleaning root...'

rm -rf default
rm -rf node_modules
rm -rf package-lock.json

cd src/client
echo 'Cleaning client...'

rm -rf default
rm -rf node_modules
rm -rf package-lock.json

cd ../server
echo 'Cleaning server...'

rm -rf default
rm -rf node_modules
rm -rf package-lock.json

echo 'Clean.'
